    "from torch import nn\n",
    "class LambdaBoy(nn.Module):\n",
    "    def __init__(self):\n",
    "        super(LambdaBoy, self).__init__()\n",
    "    def forward(self, x):\n",
    "        return lambda y: x + y"
   ]
  },
